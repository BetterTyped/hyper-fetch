{
  "name": "@hyper-fetch/devtools",
  "version": "0.0.0",
  "description": "Devtools standalone for HyperFetch",
  "author": "Maciej Pyrc <m.pyrc@interia.pl>",
  "homepage": "https://hyperfetch.bettertyped.com/",
  "license": "Apache-2.0",
  "main": ".vite/build/main.js",
  "scripts": {
    "dev": "vite",
    "clean": "npx rimraf dist",
    "test": "jest --watchAll --maxWorkers=3 --forceExit",
    "test-pipeline": "jest --watchAll=false --maxWorkers=1 --no-cache --forceExit",
    "start": "rimraf node_modules/.vite && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish:app": "electron-forge publish",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "tests": "yarn lint-staged"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.7.0",
