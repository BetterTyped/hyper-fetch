  "main": ".vite/build/main.js",
  "scripts": {
    "dev": "vite",
    "clean": "npx rimraf dist",
    "test": "jest --watchAll --maxWorkers=3 --forceExit",
    "test:pipeline": "jest --watchAll=false --maxWorkers=1 --no-cache --forceExit",
    "start": "rimraf node_modules/.vite && electron-forge start",
    "package:app": "electron-forge package",
    "make:app": "electron-forge make",
    "publish:app": "electron-forge publish",
    "release": "yarn semantic-release --extends ./release.config.js -t 'hyper-flow-v${version}'",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "typescheck": "tsc --noEmit --emitDeclarationOnly false",
    "tests": "yarn lint-staged"
  },
