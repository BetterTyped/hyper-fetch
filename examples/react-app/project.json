{
  "name": "react-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/react-app/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "run": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "react-app:app-build"
      },
      "configurations": {
        "development": {
          "buildTarget": "react-app:app-build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "react-app:app-build:production",
          "hmr": false
        }
      }
    },
    "run-server": {
      "command": "node examples/react-app/server/server.js"
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx run react-app:run", "nx run react-app:run-server"],
        "parallel": true
      }
    },
    "app-preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "react-app:app-build"
      },
      "configurations": {
        "development": {
          "buildTarget": "react-app:app-build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "react-app:app-build:production",
          "dev": false
        }
      }
    },
    "app-test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/examples/react-app"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/examples/react-app"
      }
    },
    "app-lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["examples/react-app/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
