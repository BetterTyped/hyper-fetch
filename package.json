    "call",
    "cache",
    "queue",
    "hook",
    "useFetch",
    "useCall",
    "useCache",
    "useQueue"
  ],
  "dependencies": {
    "@better-typed/react-lifecycle-hooks": "^1.0.1"
  },
  "husky": {
