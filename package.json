{
  "name": "hyper-fetch",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "electron",
      "**/electron",
      "electron/**",
      "**/electron/**"
    ]
  },
  "scripts": {
    "install-peers": "node ./scripts/install-peers",
    "prepare": "yarn install-peers && yarn husky install",
    "reset": "yarn nx reset",
    "start": "yarn reset && nx run-many --target=start --parallel=10",
    "start:app": "yarn reset && nx serve",
    "build": "yarn reset && nx run-many --target=build --parallel=10",
    "build:docs": "cd documentation && yarn install && yarn build",
    "test": "yarn reset && nx run-many --target=test-pipeline --parallel=1",
    "lint": "yarn reset && nx run-many --target=lint --parallel=10",
    "typecheck": "yarn reset && nx run-many --target=typecheck --parallel=10",
    "release": "yarn reset && nx run-many --target=release --parallel=1",
    "nx:upgrade": "npx nx migrate latest",
    "storybook:start": "nx storybook storybook --open",
    "storybook:build": "nx run storybook:build-storybook --skip-nx-cache",
    "storybook:test": "yarn start & test-storybook --url http://127.0.0.1:4400 --config-dir ./storybook/.storybook"
  },
  "dependencies": {
    "@nx/cypress": "20.3.3",
    "@nx/eslint-plugin": "20.3.3",
    "@nx/jest": "20.3.3",
    "@nx/js": "20.3.3",
    "@nx/next": "20.3.3",
    "@nx/react": "20.3.3",
    "@nx/vite": "20.3.3",
    "@nx/web": "20.3.3",
    "@nx/workspace": "20.3.3",
    "lucide-react": "^0.476.0",
    "nx": "20.3.3",
    "regenerator-runtime": "0.13.7",
    "use-immer": "^0.11.0"
  },
