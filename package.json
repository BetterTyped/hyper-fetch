    "ttypescript": "^1.5.13",
    "typescript": "~4.9.3"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BetterTyped/hyper-fetch.git"
  },
  "keywords": [
    "fetch",
    "xhr",
    "requests",
    "js",
    "graphql",
    "fetcher",
    "queue",
    "offline",
    "persistence",
    "cache",
    "builder",
    "command",
    "swr",
    "websockets",
    "ajax",
    "sse",
    "sockets",
    "events"
  ],
  "msw": {
    "workerDirectory": "src"
  },
  "peerDependencies": {
    "events": "*",
    "http": "*",
    "stream": "*"
  }
}
