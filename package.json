  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BetterTyped/hyper-fetch.git"
  },
  "keywords": [
    "fetch",
    "xhr",
    "requests",
    "js",
    "graphql",
    "fetcher",
    "queue",
    "offline",
    "persistence",
    "cache",
    "builder",
    "command"
  ],
  "msw": {
    "workerDirectory": "src"
  }
}
